 {
  "metadata": [
    {
      "src": [{
        "files": ["**/*.csproj"],
        "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**",
            "**/examples/**",
            "**/benchmark/**",
            "**/Akka.Examples.*.csproj",
            "**/*Test*.csproj",
            "**/*Test.*.csproj"
        ],
        "src": "../Akka.Hive"
      }],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "../docu/**"
        ]
      }
    ],
    "dest": "../docu",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}